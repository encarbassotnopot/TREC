    "import pandas as pd\n",
    "import json"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open(\"out_1.json\") as f:\n",
    "\ta = json.load(f)\n",
    "\n",
    "df = pd.concat([pd.DataFrame(i) for i in a]).groupby(level=0).mean()\n",
    "df.index = df.index.map(int)\n",
    "df.sort_index(inplace=True)\n",
    "\n",
    "df.to_json(\"out_1_avg.json\")"
   ]
  },
